{
  "name": "culturelense-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "web": "npm run dev -w web",
    "db:generate": "npm run generate -w web",
    "db:push": "npm run push -w web",
    "db:seed": "npm run seed -w web"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "turbo": "^1.12.4",
    "typescript": "^5.3.3"
  },
  "packageManager": "npm@10.2.0",
  "dependencies": {
    "@google/genai": "^1.34.0"
  }
}
